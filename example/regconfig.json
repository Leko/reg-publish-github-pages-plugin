{
  "core": {
    "workingDir": ".reg",
    "actualDir": "__screenshots__",
    "thresholdRate": 0.1,
    "ximgdiff": {
      "invocationType": "client"
    }
  },
  "plugins": {
    "reg-keygen-git-hash-plugin": true,
    "reg-notify-github-plugin": {
      "clientId": "$REG_NOTIFY_GITHUB_PLUGIN_CLIENT_ID"
    },
    "@lekoleko/reg-publish-github-pages-plugin": {
      "repository": "Leko/reg-publish-github-pages-plugin",
      "branchName": "reg-working-demo",
      "token": "$GITHUB_TOKEN"
    }
  }
}
